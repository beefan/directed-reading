{
  "name": "Directed Reading",
  "description" : "a directed reading assistant to enable focused attention",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "stage.html",
    "default_icon": "icon.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens stage.html"
    }
  }
}